    "ng-packagr": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.8.3",
    "webpack": "5"
  },
  "volta": {
