    "d3-sankey": "^0.12.3",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-time-format": "^4.1.0",
    "d3-transition": "^3.0.1",
    "emoji-flags": "^1.2.0",
    "moment-timezone": "^0.5.40",
